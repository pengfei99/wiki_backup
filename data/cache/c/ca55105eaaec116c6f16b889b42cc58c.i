# explicite self join
SELECT A.CustomerName AS CustomerName1, B.CustomerName AS CustomerName2, A.City
FROM Customers A inner join Customers B on A.City = B.City
WHERE A.CustomerID <> B.CustomerID
ORDER BY A.City;

# implicite self join
